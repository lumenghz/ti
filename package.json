{
  "name": "@dennislu/ti",
  "version": "0.3.0",
  "packageManager": "pnpm@7.17.1",
  "description": "Just run your task easily",
  "author": "Dennis Lu <jiahehz@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lumenghz/ti.git"
  },
  "license": "MIT",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "dist",
    "bin"
