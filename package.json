    "consola": "^2.15.3",
    "eslint": "^8.28.0",
    "esno": "^0.16.0",
    "execa": "^6.1.0",
    "fast-glob": "^3.2.12",
    "fzf": "^0.5.1",
    "prompts": "^2.4.2",
    "standard-version": "^9.5.0",
    "typescript": "^4.6.4",
    "unbuild": "^1.0.0",
    "vitest": "^0.25.3",
    "yaml": "^2.1.3",
    "yargs": "^17.6.2"
